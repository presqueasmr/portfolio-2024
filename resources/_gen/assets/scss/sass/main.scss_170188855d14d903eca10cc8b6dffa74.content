body{margin:0 auto;padding:0;font-family:sans-serif;height:100vh;width:100vw;background-color:#f1f1f1;color:#323242}h1,h2,h3,h4,h5,h6,p,li,ul,ol{padding:0;margin:0;font-weight:400}h1{font-size:4rem;font-family:manrope,sans-serif}h2{font-size:2rem;font-family:manrope,sans-serif;margin-bottom:1rem}h3{font-family:dotgothic16,sans-serif;font-size:1rem;margin-bottom:.5rem;opacity:.65}h4{font-family:dotgothic16,sans-serif;font-size:1rem}h5{font-family:dotgothic16,sans-serif;font-size:.875rem;opacity:.65}p,li{font-family:manrope,sans-serif}a,span{font-family:dotgothic16,sans-serif}p{font-size:1rem;letter-spacing:.25px}li{font-size:1.125rem}a{text-decoration:none;color:#323242}.footer,.header{list-style:none;display:flex;justify-content:space-between;align-items:center;padding:0 4rem}.footer span,.header span{font-size:.875rem}.footer .logo_container,.header .logo_container{position:absolute;left:calc(50vw - (90px/2))}.footer .logo_container img,.header .logo_container img{width:25px;height:25px}@media screen and (max-width:600px){.footer,.header{padding:0 2rem}}.footer.footer__container-sticky,.header.footer__container-sticky{position:sticky;padding:0 0 0 2rem;background-color:#f4f4f5;bottom:10px;left:120px;width:calc(100vw - 240px - 2rem);z-index:2;border-radius:50px;outline:1.8px solid #323242d1;border:none;box-shadow:1px 1px 4px #00000032}@media screen and (max-width:900px){.footer.footer__container-sticky,.header.footer__container-sticky{left:20px;width:calc(100vw - 40px - 2rem);font-size:.875rem}.footer.footer__container-sticky li,.header.footer__container-sticky li{font-size:.875rem;height:45px;vertical-align:middle}}.external-link{padding:.75rem 2rem;border-radius:50px;outline:1.5px solid #323242d1;display:flex;align-items:center}.external-link::after{content:' ';display:block;align-self:baseline;margin-left:.75rem;width:24px;aspect-ratio:1/1;background-image:url(/icon/external-link.svg);background-repeat:no-repeat}.header{border-bottom:1px solid #323242}.footer{border-top:1px solid #323242;position:fixed;bottom:0;width:calc(100% - 8rem)}.project_wrapper{height:80vh;display:flex;overflow:hidden;gap:20px;position:absolute;left:50%;top:40%;transform:translate(0%,-30%)}.project_wrapper__container{display:flex;flex-direction:column;gap:1rem;height:100%;width:300px}.project_wrapper__container h2{margin-bottom:.2rem}.project_wrapper__container .img-wrapper{object-fit:cover;object-position:90% 50%;aspect-ratio:1/1}.project_wrapper__tag-container,.project_wrapper__date-container{display:flex;justify-content:space-between;align-items:baseline}.project_wrapper__tag-container h3,.project_wrapper__date-container h3{width:max-content}ul{list-style:none}img{min-width:70px;min-height:70px}@media screen and (max-width:900px){img{min-width:65px;min-height:65px}}.section-1,section:nth-child(6){margin-top:6rem;text-align:center;display:flex;flex-direction:column;align-items:center}.section-1 .title,section:nth-child(6) .title{padding:0 25%}.section-1 .title ul,section:nth-child(6) .title ul{display:flex;justify-content:center;gap:32px;font-size:.75rem}.section-1 .external-link{margin:2rem 0}.section-1 .cover_image{aspect-ratio:16/9;width:50%;z-index:2}.section-1 .cover_image img{object-fit:cover;width:100%}.section-1 .problem__container{margin-top:-2rem;z-index:0;background-color:#dfff85;padding:4rem 0;font-size:1rem;min-width:100vw}.section-1 .problem__container>h3,.section-1 .problem__container>p{padding:0 25%}section:nth-child(3),section:nth-child(4),section:nth-child(5),section:nth-child(6){margin:10rem 60px}.think_container{display:flex}.think_container div{width:50%}.think_container div h2{margin-bottom:3rem}.think_container div li{padding:1rem .5rem;border-bottom:solid 1px #323242}.think_container div .solutions::before,.think_container div .pains::before{content:'';width:20px;padding:15px;display:inline;background-image:url(/icon/check.svg);background-position:50%;background-repeat:no-repeat}.think_container div .solutions::before{background-image:url(/icon/check.svg)}.think_container div .pains::before{background-image:url(/icon/cross.svg)}@media screen and (max-width:600px){.think_container{flex-direction:column}.think_container div{width:100%;margin-top:20px}}.process__wrapper{display:flex;justify-content:space-between;flex-wrap:wrap;overflow:visible;gap:32px}.process__wrapper .process__item{width:48.5%;display:flex;flex-direction:column}.process__wrapper .process__item img{width:100%;aspect-ratio:16/10;object-fit:cover;object-position:center;margin-bottom:4rem}.process__wrapper .process__item .process__text{display:flex;align-items:center;margin-bottom:2rem;gap:16px}.process__wrapper .process__item .process__text .percentage{max-width:70px;aspect-ratio:1/1;margin-bottom:0}@media screen and (max-width:796px){.process__wrapper{gap:32px;flex-direction:column}.process__wrapper .process__item{width:100%}}section:nth-child(5) h2{margin-bottom:3rem}.gallery__container h4{font-size:14px;font-weight:400;margin-bottom:1rem}.gallery__container .top{display:flex;gap:20px}.gallery__container .top>div{display:block}.gallery__container .top>:first-child img{margin:1rem 0}.gallery__container .top .center-side div img,.gallery__container .top .right-side div img{width:100%;aspect-ratio:16/9}.gallery__container .top .right-side{width:30%;display:flex;flex-direction:column;justify-content:space-between;gap:.25rem}.gallery__container .top .center-side{width:60%;display:flex;flex-direction:column;justify-content:space-between;gap:1rem}.gallery__container .top .left-side{width:10%}.gallery__container .top .left-side div{display:flex;flex-direction:column}@media screen and (max-width:600px){.gallery__container .top .left-side div{flex-direction:row;gap:20px}}@media screen and (max-width:600px){.gallery__container .top{flex-direction:column}}.gallery__container .bottom{display:flex;gap:20px}.gallery__container .bottom .assets-container>div{display:flex}.gallery__container .bottom .assets-container>div img{aspect-ratio:9/16!important;width:25%}section:nth-child(6){padding:2rem 0;background-color:#dfff85}section:nth-child(6) ul{width:50%}section:nth-child(6) li{padding:.5rem;text-align:left}.btn-next{padding:1rem 0;margin:2rem 60px;display:flex;justify-content:space-between;align-items:center;border-bottom:solid 1px #323242}.btn-next img{width:20px!important;height:20px!important}